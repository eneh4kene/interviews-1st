{
    "name": "@interview-me/ui",
    "version": "0.0.0",
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./button": {
            "types": "./dist/components/ui/button.d.ts",
            "import": "./dist/components/ui/button.mjs",
            "require": "./dist/components/ui/button.js"
        },
        "./input": {
            "types": "./dist/components/ui/input.d.ts",
            "import": "./dist/components/ui/input.mjs",
            "require": "./dist/components/ui/input.js"
        },
        "./card": {
            "types": "./dist/components/ui/card.d.ts",
            "import": "./dist/components/ui/card.mjs",
            "require": "./dist/components/ui/card.js"
        },
        "./badge": {
            "types": "./dist/components/ui/badge.d.ts",
            "import": "./dist/components/ui/badge.mjs",
            "require": "./dist/components/ui/badge.js"
        },
        "./select": {
            "types": "./dist/components/ui/select.d.ts",
            "import": "./dist/components/ui/select.mjs",
            "require": "./dist/components/ui/select.js"
        },
        "./label": {
            "types": "./dist/components/ui/label.d.ts",
            "import": "./dist/components/ui/label.mjs",
            "require": "./dist/components/ui/label.js"
        },
        "./alert": {
            "types": "./dist/components/ui/alert.d.ts",
            "import": "./dist/components/ui/alert.mjs",
            "require": "./dist/components/ui/alert.js"
        },
        "./styles.css": "./dist/styles.css"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint .",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-slot": "^1.0.2",
        "@types/jsonwebtoken": "^9.0.10",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.294.0",
        "tailwind-merge": "^2.0.0",
        "tsup": "^8.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/react": "^18.2.45",
        "@types/react-dom": "^18.2.18",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "postcss": "^8.4.32",
        "react": "^18.2.0",
        "tailwindcss": "^3.3.6",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
